{
  "name": "@fluentui/jest-make-styles",
  "version": "9.0.0-beta.1",
  "description": "Jest utilities for makeStyles",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fluentui"
  },
  "license": "MIT",
  "scripts": {
    "build": "just-scripts build --commonjs",
    "lint": "just-scripts lint",
    "test": "jest",
    "just": "just-scripts",
    "clean": "just-scripts clean",
    "code-style": "just-scripts code-style",
    "docs": "api-extractor run --config=config/api-extractor.local.json --local",
    "build:local": "tsc -p . --module esnext --emitDeclarationOnly && node ../../scripts/typescript/normalize-import --output ./dist/packages/jest-make-styles/src && yarn docs"
  },
  "devDependencies": {
    "@fluentui/eslint-plugin": "*",
    "@fluentui/scripts": "^1.0.0",
    "@fluentui/react-make-styles": "9.0.0-beta.1",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "24.9.1",
    "enzyme": "~3.10.0",
    "enzyme-adapter-react-16": "^1.15.0",
    "@types/react": "16.9.42",
    "@types/react-dom": "16.9.10",
    "@types/react-test-renderer": "^16.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-test-renderer": "^16.3.0"
  },
  "dependencies": {
    "@fluentui/make-styles": "9.0.0-beta.1",
    "jest-diff": "^24.9.0"
  },
  "beachball": {
    "tag": "beta",
    "disallowedChangeTypes": [
      "major",
      "minor",
      "patch"
    ]
  }
